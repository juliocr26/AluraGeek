{
  "functions": {
    "api/server.js": {
      "includeFiles": "db.json"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "api/server.js"
    },
    {"source": "/admin/", "destination": "https://admin-app.vercel.com/"},
    {"source": "/admin/:match*", "destination": "https://admin-app.vercel.com/:match*"},
    {"source": "/", "destination": "https://alura-geek-cyan-two.vercel.app/"},
    {"source": "/:match*", "destination": "https://alura-geek-cyan-two.vercel.app/:match*"}
  ]
}

